{
  "name": "crud-electron",
  "version": "1.0.0",
  "description": "A Simple CRUD Operation in Electron",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --asar --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --asar",
    "package-linux": "electron-packager . --asar --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "keywords": [
    "electron",
    "app",
    "crud",
    "operation"
  ],
  "author": "Rajon Kobir",
  "license": "ISC",
  "devDependencies": {
    "electron": "^9.0.0",
    "electron-packager": "^14.2.1"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RajonKobir/crud-electron.git"
  },
  "bugs": {
    "url": "https://github.com/RajonKobir/crud-electron/issues"
  },
  "homepage": "https://github.com/RajonKobir/crud-electron#readme"
}
